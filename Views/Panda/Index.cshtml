<table class="table table-striped">
    <tr>
        <th>User</th>
        <th>Alias</th>
        <th>Username</th>
        <th>Created At</th>        
    </tr>

@foreach(var user in @ViewBag.allusers)
 {
    <tr>
        <td>@user.name <td>
        <td>@user.alias</td>
        <td>@user.username</td>
        <td>@user.created_at.ToString("D")</td>
    </tr>
}
</table>
